; Grace in assembly (x86_64 Linux version)%1$c%1$c%%define CODE %2$s%1$c%%define FILE \"Grace_kid.s\"%1$c%%define O_FLAGS 0x241%1$c%1$csection .data%1$cfilename db FILE,0%1$ccode db CODE,0%1$ccode_len equ $ - code%1$c%1$csection .text%1$cglobal _start%1$c_start:%1$c    mov rax, 2%1$c    mov rdi, filename%1$c    mov rsi, O_FLAGS%1$c    mov rdx, 0o644%1$c    syscall%1$c    cmp rax, 0%1$c    jl exit%1$c    mov r8, rax%1$c    mov rax, 1%1$c    mov rdi, r8%1$c    mov rsi, code%1$c    mov rdx, code_len%1$c    syscall%1$c    mov rax, 3%1$c    mov rdi, r8%1$c    syscall%1$cexit:%1$c    mov rax, 60%1$c    xor rdi, rdi%1$c    syscall%1$c